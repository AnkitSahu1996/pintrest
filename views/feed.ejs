<% include ./partials/header.ejs %> <% include ./partials/nav.ejs %>
<div class="w-full min-h-screen bg-zinc-800 px-10 py-5 text-white">
  <div class="cards flex flex-wrap gap-10">
    <% posts.forEach((e)=>{ %>
    <div class="card w-40">
      <div class="w-full h-40 rounded-lg overflow-hidden">
        <img class="w-full h-full object-cover" src="/images/uploads/<%= e.image %>" alt="error 404" />
      </div>
      <h5 class="mt-2 text-sm"><%= e.title %></h5>
      <h6 class="text-xs opacity-500 font-semibold"><%= e.user.username %></h6>
    </div>
    <% }) %>
  </div>
</div>
<% include ./partials/footer.ejs %>
